<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liuuuu.sharedb">

    <uses-permission android:name="com.liuuuu.datapersistdemo.permission.READ_PREFERENCE" />
    <uses-permission android:name="com.liuuuu.datapersistdemo.permission.WRITE_PREFERENCE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SharedbActivity" />
        <activity android:name=".SharespActivity" />
        <activity android:name=".ShareOtherActivity"></activity>
    </application>

</manifest>